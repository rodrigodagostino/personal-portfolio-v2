{
  "bracketSpacing": true,
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "plugins": ["prettier-plugin-astro"],
  "pluginSearchDirs": ["."],
  "overrides": [
    {
      "files": ["*.json", "*.scss", "*.css"],
      "options": { "singleQuote": false }
    },
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
